@page
@model ProjectStation.Pages.Exhibition.TigSummer2020Model
@{
    ViewData["Title"] = "tigSummer2020";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="fullMediumHeader">I_AM_BECOME.pdf Michael Ho</div>
            <div class="fadedLine"></div>
            <div id="vrview"></div>
        </div>
    </div>

    <div class="row">
        @foreach (var artPiece in Model.ArtPieces)
        {
            <div class="col-md-6">
                <a asp-page="/Exhibition/Works" asp-route-id=@artPiece.Id><img class="img-fluid"src="@Url.Content(artPiece.PhotoPath)" /></a>
                <p>@artPiece.Name</p>
            </div>
        }
    </div>


    @*@foreach (var artPiece in Model.ArtPieces)
